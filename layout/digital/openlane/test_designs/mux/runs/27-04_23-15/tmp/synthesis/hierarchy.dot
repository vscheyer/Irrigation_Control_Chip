rankdir="LR";
remincross=true;
n2 [ shape=diamond, label="address", color="black", fontcolor="black" ];
n3 [ shape=diamond, label="inputs", color="black", fontcolor="black" ];
n4 [ shape=octagon, label="in3", color="black", fontcolor="black" ];
